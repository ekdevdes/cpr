{
  "name": "cpr-node",
  "version": "0.0.1",
  "description": "A command-line website creation toolset. CPR stands for Create, Package, Run. CPR helps you create the initial file structure for your website, acts a basic package manager and parses HTML, CSS and JS pre-processor languages such as Markdown or HAML, SASS or LESS and CoffeeScript",
  "author": "Ethan Kramer <ethankr@me.com> (http://ek.alphaschildren.org)",
  "engines": {
    "node": ">=0.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekdevdes/cpr.git"
  },
  "bugs": {
    "url": "https://github.com/ekdevdes/cpr/issues"
  },
  "keywords": [
    "website",
    "creation",
    "cli",
    "command",
    "line",
    "haml",
    "sass",
    "markdown",
    "less",
    "coffeescript",
    "js",
    "javascript",
    "html",
    "pre-processor",
    "package",
    "manager",
    "create",
    "toolset",
    "parse"
  ],
  "dependencies": {
    "chalk": "~>0.5.1",
    "cmd-exec": "~>0.1.0",
    "optimist": "~>0.6.1",
    "github-markdown": "~>0.5.0",
    "jslint": "~>0.10.0",
    "less-creeper": "~>0.0.2",
    "nodewatch": "~>0.3.1",
    "request": "~>2.82.0",
    "yamljs": "^0.2.1",
    "semver": "4.3.2",
    "mkdirp": "0.5.0",
    "snyk": "^1.82.2"
  },
  "bin": {
